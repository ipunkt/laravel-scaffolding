{
	"name": "ipunkt/laravel-package",
	"description": "This is a starter package for laravel",
	"type": "library",
	"require": {
		"php": ">=7.0.0",
		"illuminate/support": "^5.5",
		"ipunkt/laravel-package-manager": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Ipunkt\\LaravelPackage\\": "src/"
		}
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Robert Kummer",
			"email": "rok@ipunkt.biz"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"extra": {
		"laravel": {
			"dont-discover": [
			],
			"providers": [
				"Ipunkt\\LaravelPackage\\Providers\\LaravelPackageServiceProvider"
			],
			"aliases": {
			}
		}
	}

}
